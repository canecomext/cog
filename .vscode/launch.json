  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch generator",
      "type": "node",
      "program": "${workspaceFolder}/src/cli.ts",
      "cwd": "${workspaceFolder}/example",
      "env": {},
      "runtimeExecutable": "/Users/zgobolos/.deno/bin/deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-all"
      ],
      "args": [
        "--modelsPath",
        "./models",
        "--outputPath",
        "./generated",
        "--no-softDeletes"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "Launch example",
      "type": "node",
      "program": "${workspaceFolder}/example/example.ts",
      "cwd": "${workspaceFolder}/example",
      "env": {},
      "runtimeExecutable": "/Users/zgobolos/.deno/bin/deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-all"
      ],
      "attachSimplePort": 9229
    }
  ]
}
